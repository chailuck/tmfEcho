package log_test

import (
	"errors"
	"omr/api/log"
	"testing"

	"github.com/stretchr/testify/assert"
)

func TestEndResult(t *testing.T) {
	assert.Equal(t, []string{"a", "b", "200"}, log.EndResult("a", "b", nil))
	assert.Equal(t, []string{"a", "b", "500", "error"}, log.EndResult("a", "b", errors.New("error")))
}
