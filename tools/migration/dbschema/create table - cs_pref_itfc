CREATE TABLE cs_pref_itfc (
seqn SERIAL NOT NULL,
cust_numb integer NOT NULL,
subr_numb char(12) NOT NULL,
sms_lang char(2) NOT NULL,
ivr_lang char(2) NOT NULL,
ussd_lang char(2) NOT NULL,
trns_stts char(1) NOT NULL,
crtd_dttm TIMESTAMP(0) NOT NULL,
crtd_by char(12) NOT NULL,
last_chng_dttm TIMESTAMP(0) NOT NULL,
last_chng_by char(12) NOT NULL
)

// Oracle date_of_brth
CREATE TABLE cs_pref_itfc (
seqn numeric GENERATED BY DEFAULT AS IDENTITY PRIMARY KEY,
cust_numb integer NOT NULL,
subr_numb char(12) NOT NULL,
sms_lang char(2) NOT NULL,
ivr_lang char(2) NOT NULL,
ussd_lang char(2) NOT NULL,
trns_stts char(1) NOT NULL,
crtd_dttm TIMESTAMP(0) NOT NULL,
crtd_by char(12) NOT NULL,
last_chng_dttm TIMESTAMP(0) NOT NULL,
last_chng_by char(12) NOT NULL
)